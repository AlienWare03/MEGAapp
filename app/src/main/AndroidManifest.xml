<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.projectmega.menu">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ProfileActivity" />
        <activity
            android:name=".Register"
            android:label="Register" />
        <activity
            android:name=".Navigation"
            android:label="@string/title_activity_navigation"
            android:parentActivityName=".MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.projectmega.menu.MainActivity" />
        </activity>
        <activity
            android:name=".Subjects"
            android:label="Subjects" />
        <activity
            android:name=".Exam"
            android:label="Exam Scores"
            android:theme="@style/AppTheme1" />
        <activity
            android:name=".UpdateAccount"
            android:label="Update Account Info"
            android:theme="@style/AppTheme1">

        </activity>
        <activity
            android:label="Grade"
            android:theme="@style/AppTheme1"
            android:name=".FinalGrades">


        </activity>
    </application>

</manifest>
